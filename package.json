    "date-fns": "^2.30.0",
    "i18next": "^22.4.9",
    "lodash": "^4.17.21",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-globe.gl": "2.22.10",
    "react-hook-form": "^7.43.1",
